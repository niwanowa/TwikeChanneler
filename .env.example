# AWS設定
AWS_REGION=ap-northeast-1
AWS_PROFILE=default

# Slack設定
SLACK_BOT_TOKEN=xoxb-your-token
SLACK_SIGNING_SECRET=your-signing-secret

# デフォルトチャンネル設定（チャンネル振り分けに失敗した場合のフォールバック）
DEFAULT_SLACK_CHANNEL=general

# AIサービス設定（将来的な拡張用）
AI_SERVICE_API_KEY=your-api-key
AI_SERVICE_ENDPOINT=https://api.example.com/v1/analyze

# デバッグ設定
DEBUG=false
LOG_LEVEL=INFO 